{
  "name": "wingmall",
  "version": "3.41.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --port=8081",
    "start-ios": "node node_modules/react-native/local-cli/cli.js start --port=8080",
    "start-android": "node node_modules/react-native/local-cli/cli.js start --port=8080",
    "test": "jest",
    "bundle-app": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build-android-release": "cd ./android && ./gradlew clean && cd .. && npx jetifier && cd ./android && ./gradlew app:assembleRelease && ./gradlew installRelease",
    "build-android-bundle-release": "cd ./android && ./gradlew clean && cd .. && npx jetifier && cd ./android && ./gradlew bundleRelease",
    "build-android-debug": "cd ./android && ./gradlew clean && cd .. && npx jetifier && cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "ios-clean": "rm -rf ./ios/Pods && npm run pod",
    "android-clean": "cd ./android && ./gradlew clean && cd ..",
    "clean": "rm -rf ./node_modules && npm i",
    "pod": "cd ios && pod install && cd ..",
    "use-stg": "cp fir-files/IOS-Staging.plist ios/GoogleService-Info.plist && cp fir-files/Android-Staging.json android/app/google-services.json && cp env/stg-env.js env/index.js",
    "use-prd": "cp fir-files/IOS-Production.plist ios/GoogleService-Info.plist && cp fir-files/Android-Production.json android/app/google-services.json & cp env/prd-env.js env/index.js",
    "push-ios-staging": "appcenter codepush release-react -a muuve/muuve-customer-ios --plist-file ios/food/Info.plist -d Staging -t 3.42.2",
    "push-ios-production": "appcenter codepush release-react -a muuve/muuve-customer-ios --plist-file ios/food/Info.plist -d Production -t 3.42.1",
    "push-android-staging": "appcenter codepush release-react -a muuve/muuve-android -d Staging",
    "push-android-production": "appcenter codepush release-react -a muuve/muuve-android -d Production",
    "remove-apks": "rm android/app/build/outputs/bundle/release/app.apks",
    "aab-to-apks": "bundletool build-apks --bundle=android/app/build/outputs/bundle/release/app-release.aab --output=android/app/build/outputs/bundle/release/app.apks --ks=android/app/muuve-keystore.keystore --ks-key-alias=muuve-keystore",
    "install-apks": "bundletool install-apks --apks=android/app/build/outputs/bundle/release/app.apks",
    "android-install-apks": "npm run aab-to-apks && npm run install-apks",
    "postinstall": "patch-package && git checkout node_modules/react-native-share/package.json"
  },
